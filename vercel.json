{
  "name": "broker-listing",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "server.js",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      },
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static",
      "config": {
        "includeFiles": [
          "src/public/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/((?!.*\\.\\w+$).*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.+\\.[a-z]+)$",
      "dest": "/public/$1"
    }
  ]
}